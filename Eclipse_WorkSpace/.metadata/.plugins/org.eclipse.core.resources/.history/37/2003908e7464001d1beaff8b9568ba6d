package messageHandler;

import MainSystem.SettingsController;

public class ConsoleHandler{
	public static int showHowMany = 5;
	public static String getConsole() {
		int size = AllMessages.size();
		size --;
		System.out.println("CONSOLE: ");
		for(int i = size; i < size-showHowMany; i++) {
			if(SettingsController.getSetting("Date/TimeSet").equals("true")) {
				System.out.println(AllMessages.AllMessagesT.get(i));
			}else {
				System.out.println(AllMessages.AllMessages.get(i));
			}
		}
		return "";
	}
}